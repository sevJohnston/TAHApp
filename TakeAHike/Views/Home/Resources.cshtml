@model IEnumerable<TakeAHike.Models.Resource>
@{
    Layout = "_NewLayout";
    ViewData["Title"] = "Hiking Groups and Area Experts";
}


<body>
    <div class="panel-body">
        <h1>@ViewData["Title"]</h1>

        <table class="table table-sm table-responsive table-striped table-bordered">
            <thead>
                <tr>
                    <th>People and Hiking Groups</th>
                    <th>Links</th>
                    <th>Description</th>

                </tr>
            </thead>
            <tbody>
                @foreach (TakeAHike.Models.Resource r in Model)
                {
                    <tr>
                        <td>@r.ResourceName</td>
                        <td><a href="@r.Link">@r.Link</a></td>
                        <td>@r.Description</td>
                    </tr>

                }
            </tbody>
        </table>
    </div>



</body>
